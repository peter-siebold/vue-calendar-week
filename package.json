{
  "name": "roadsurfer-frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build --emptyOutDir",
    "preview": "vite preview",
    "prettier": "prettier --write '**/*.{ts,js,json,md,vue}'",
    "start:server": "node server.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "deploy": "sh deploy.sh"
  },
  "dependencies": {
    "@types/moment": "^2.13.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "moment": "^2.29.2",
    "vue": "^3.2.25",
    "vue-router": "^4.0.14",
    "vue-uuid": "^3.0.0",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/compiler-sfc": "3.2.31",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^8.6.0",
    "jest": "^27.5.1",
    "postcss": "^8.4.12",
    "prettier": "2.6.2",
    "sass": "^1.50.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.5.4",
    "vite": "^2.9.2",
    "vue-tsc": "^0.29.8"
  }
}
